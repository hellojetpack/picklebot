{
  "name": "picklebot",
  "version": "0.0.1",
  "description": "A slack bot that manages your companies' pickleball games",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "test": "tape -r babel-register components/**/*_spec.js | tap-spec",
    "startDev": "nodemon -L bot.js --exec babel-node",
    "lt": "node components/localtunnel.js",
    "develop": "npm run lt & npm run startDev"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "botkit": "^0.5",
    "botkit-storage-mongo": "^1.0.4",
    "dashbot": "0.7.4",
    "debug": "^2.3.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "localtunnel": "^1.8.2",
    "moment": "^2.18.1",
    "querystring": "^0.2.0",
    "request": "^2.79.0",
    "supertest": "^3.0.0",
    "tape": "^4.6.3",
    "wordfilter": "^0.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellojetpack/picklebot.git"
  },
  "keywords": [
    "bots",
    "chatbots",
    "slack"
  ],
  "author": "hellojetpack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hellojetpack/picklebot/issues"
  },
  "homepage": "https://github.com/hellojetpack/picklebot#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0",
    "tap-spec": "^4.1.1"
  }
}
